@model List<Microsoft.WindowsAzure.Storage.Blob.CloudBlockBlob>

@{
    ViewBag.Title = "Index";
}

@foreach (var item in Model)
{
    <img src="@item.Uri" style="max-width: 300px;"/>
    <br/>
    <a href="/Fotos/Borrar?id=@item.Name">Borrar</a>
    <br />
}

<div>
    <button  onclick="@("window.location.href='" + @Url.Action("Nueva") + "'")">Subir sosio</button>
    @*@Html.ActionLink("Subir nueva foto", "Nueva")*@
</div>